# Uncomment this line to define a global platform for your project
platform :ios, '13.0'

# CocoaPods analytics sends network stats synchronously affecting flutter build latency.
ENV['COCOAPODS_DISABLE_STATS'] = 'true'

require File.expand_path('../.flutter-plugins', __FILE__)
require File.expand_path('../.flutter-plugins-dependencies', __FILE__)
require File.expand_path('../Flutter/ephemeral/.symlinks/flutter/dart_tools/bin/podhelper.rb', __FILE__)

target 'Runner' do
  use_frameworks!
  use_modular_headers!

  flutter_install_all_ios_pods File.dirname(File.realpath(__FILE__))
end
